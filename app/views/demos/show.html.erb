<div class="main demos-show">
  <div class="container">
    <div class="demos-show-item">
      <p>
        <%= @demo.name %>
        <%= @demo.univercity_name %>
        <%= @demo.team_name %>
      </p>
      <p>
        <%= @demo.comment %>
      </p>      
      <!-- ここに指定されたHTMLを貼り付けてください -->
      <div class="demo-time">
        <!-- @demoのcreated_atカラムのデータを表示してください -->
        <%= @demo.created_at %>
      </div>
      <div class="demo-menus">
          <%=link_to("編集", "/demos/#{@demo.id}/edit")%>
          <%=link_to("削除", "/demos/#{@demo.id}/destroy", {method:"demo"})%>
      </div>
    </div>
  </div>
</div>